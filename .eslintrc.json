{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 10
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unsafe-optional-chaining": "off"
    }
}
